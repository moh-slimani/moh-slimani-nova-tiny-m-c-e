{
    "name": "moh-slimani/nova-tiny-m-c-e",
    "description": "A Laravel Nova field that uses Tiny MCE.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Slimani",
            "email": "mohamed@slimani.dev"
        }
    ],
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "MohSlimani\\NovaTinyMCE\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MohSlimani\\NovaTinyMCE\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
